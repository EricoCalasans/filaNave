
        <div id="preloader">
            <div id="status"><i class="icon-spinner icon-spin"></i></div>
        </div>
        
        <header class="dark_grey"> <!-- Header start -->
            <a href="#" class="logo_image"><span class="hidden-480"></span></a>
            <ul class="header_actions pull-left hidden-480 hidden-768">
                <li rel="tooltip" data-placement="bottom" title="" data-original-title="Ocultar/Exibir o menu"><a href="#" class="hide_navigation"><i class="icon-chevron-left"></i></a></li>
            </ul>

            <ul class="header_actions">

                                <li rel="tooltip" data-placement="bottom" title="" class="messages" id="barra_novas_mensagens" data-original-title="0 novas mensagens">
                    <a class="iconic" href="#"><i class="icon-envelope-alt"></i> 0</a>
                    <ul class="dropdown-menu pull-right messages_dropdown">
                        
                        
                        <a href="https://san.navedoconhecimento.org.br/admin/mp/" class="btn btn-block blue align_left"><span>Central de Mensagens</span></a>
                    </ul>
                </li>

                <li class="dropdown"><a href="#"> ÉRICO CALASANS <i class="icon-angle-down"></i></a>
                    <ul>
                        <li><a href="https://san.navedoconhecimento.org.br/admin/funcionario/editar"><i class="icon-cog"></i> Minha conta</a></li>
                    </ul>
                </li>
                <li><a href="https://san.navedoconhecimento.org.br/admin/login/logout"><i class="icon-signout"></i> <span class="hidden-768 hidden-480">Sair</span></a></li>
                <li class="responsive_menu"><a class="iconic" href="#"><i class="icon-reorder"></i></a></li>
            </ul>
        </header>

        <div id="main_navigation" class="dark_navigation"> <!-- Main navigation start -->
            <div class="inner_navigation mCS_destroyed" style="height: inherit;">

                <ul class="main">
                    <li><a href="https://san.navedoconhecimento.org.br/admin/painel"><i class="icon-home"></i>Painel</a></li>
                </ul>
                <ul class="main"><li><a href="https://san.navedoconhecimento.org.br/admin/responsavel" class="expand subClosed"><i class="icon-eye-open"></i>Responsáveis<span class="count"><i class="icon-chevron-down"></i></span></a><ul class="sub_main" style="display: none;"><li><a href="https://san.navedoconhecimento.org.br/admin/responsavel/index"><i class=""></i>Listar responsáveis</a></li></ul></li><li><a href="https://san.navedoconhecimento.org.br/admin/espaco" class="expand subClosed"><i class="icon-screenshot"></i>Espaços<span class="count"><i class="icon-chevron-down"></i></span></a><ul class="sub_main" style="display: none;"><li><a href="https://san.navedoconhecimento.org.br/admin/espaco/index"><i class=""></i>Listar espaços</a></li></ul></li><li><a href="https://san.navedoconhecimento.org.br/admin/evento" class="expand subClosed"><i class="icon-book"></i>Formações<span class="count"><i class="icon-chevron-down"></i></span></a><ul class="sub_main" style="display: none;"><li><a href="https://san.navedoconhecimento.org.br/admin/evento/index"><i class=""></i>Listar formações</a></li><li><a href="https://san.navedoconhecimento.org.br/admin/evento/inscritos"><i class=""></i>Listar Inscritos</a></li></ul></li><li><a href="https://san.navedoconhecimento.org.br/admin/mp"><i class="icon-envelope-alt"></i>Central de mensagens</a></li></ul>

            </div>
        </div>  

        <script src="https://san.navedoconhecimento.org.br/assets/admin/js/library/bootstrap-datetimepicker.js"></script>
<script src="https://san.navedoconhecimento.org.br/assets/admin/js/library/bootstrap-timepicker.js"></script>
<script src="https://san.navedoconhecimento.org.br/assets/admin/js/library/bootstrap-datepicker.js"></script>

<script src="https://san.navedoconhecimento.org.br/assets/admin/js/datatables.js"></script>
<script type="text/javascript">

    jQuery.noConflict();

    jQuery(function($) {
        

        $('.modal').bind("show", function(){
            $(this).desativar_recarregamento();
        });
        
        $('.modal').bind("hide", function(){
            $(this).reiniciar_recarregamento();
        });
        

        $('#limite_tempo').timepicker({
            minuteStep: 1,
            showSeconds: true,
            showMeridian: false,
            defaultTime: 'value'
        });

        //omite todas as listagens de cada espaço
        $(".listagem_espaco").hide();
        $(".icon-minus-sign").hide();

        //carrega o código relativo à alteração de estado, utilizado por existir
        //interação via ajax e ser necessário o recarregamento por várias vezes
        $.fn.carregar_dados_solicitacao = function() {

            $("[rel='tooltip']").tooltip('destroy');

            $(".limpar_canceladas").on("click", function() 
            {
                espaco_limpeza = $(this).data('espaco');
                $('.link_limpar_canceladas').data('espaco', espaco_limpeza);
            });
            
            $('.table').delegate(".alterar_estado", "click", function() {

                espaco = $(this).data('espaco');
                estado = $(this).data('estado');
                solicitacao = $(this).data('solicitacao');
                frequentador = $(this).data('frequentador');
                operacao_solicitacao = 'atendimento';

            });
            
            $('.table').delegate(".trocar_espaco", "click", function() {

                solicitacao_troca = $(this).data('solicitacao');
                espaco_origem = $(this).data('espaco-origem');
                $('.link_troca_espaco').data('solicitacao', solicitacao_troca);
                $('.link_troca_espaco').data('espaco-origem', espaco_origem);

            });
            
            $('.table').delegate(".trocar_equipamento", "click", function() {

                equipamento = $(this).data('equipamento');
                espaco = $(this).data('espaco');
                solicitacao = $(this).data('solicitacao');
                operacao_solicitacao = 'troca_equipamento';

                $('.enviar_alteracao').data('equipamento', equipamento);

            });

            //aciona a janela de confirmação da alteração de tempo 
            //configurando os dados que habilitam a alteração
            $('.table').delegate('.alterar-tempo', 'click', function() {
                solicitacao_equipamento = $(this).data('equipamento');
                solicitacao_tempo = $(this).data('solicitacao');
                espaco_tempo = $(this).data('espaco');

                $('.link_alteracao_tempo').data('equipamento', solicitacao_equipamento);
                $('.link_alteracao_tempo').data('solicitacao', solicitacao_tempo);
                $('.link_alteracao_tempo').data('espaco', espaco_tempo);
         
            });
            
            //aciona a janela de confirmação da finalização de tempo 
            //configurando os dados que habilitam a finalização
            $('.table').delegate('.finalizar-tempo', 'click', function() {
                solicitacao_equipamento = $(this).data('equipamento');
                solicitacao_tempo = $(this).data('solicitacao');
                espaco_tempo = $(this).data('espaco');

                $('.link_finalizacao_tempo').data('equipamento', solicitacao_equipamento);
                $('.link_finalizacao_tempo').data('solicitacao', solicitacao_tempo);
                $('.link_finalizacao_tempo').data('espaco', espaco_tempo);

            });     
            
            //aciona a janela de cancelamento de utilização 
            //configurando os dados que habilitam o cancelamento
            $('.table').delegate('.cancelar-utilizacao', 'click', function() {
                
                solicitacao_cancelamento = $(this).data('solicitacao');
                espaco_cancelamento = $(this).data('espaco');

                $('.link_cancelamento_utilizacao').data('solicitacao', solicitacao_cancelamento);
                $('.link_cancelamento_utilizacao').data('espaco', espaco_cancelamento);

            });
            
            $("[rel='tooltip']").tooltip();

        };
        
        var permissao_solicitacao = true;

        //exibe/omite a listagem de um determinado espaço
        $(".gatilho_espaco").on("click", function() {
            if (permissao_solicitacao)
            {
                var espaco = $(this).data('espaco');

                $("#gatilho_espaco_" + espaco + " .icon-plus-sign").toggle();
                $("#gatilho_espaco_" + espaco + " .icon-minus-sign").toggle();
                $("#listagem_espaco_" + espaco).slideToggle(function(){

                    if ($(this).is(':visible'))
                    {
                        $(this).find('.loading').show();
                        $(this).find('.dataTables_empty').html('Carregando dados da tabela...');
                        //$(this).ativar_recarregamento(espaco);
                        $(this).carregar_dados_solicitacao();
                        $(this).popular_listas(espaco, true);
                    }
                    else
                    {
                        $(this).desativar_recarregamento(espaco);
                    }

                });
            }
            else {
                $.pnotify({
                    title: "Você não possui permissão para atender solicitações.",
                    type: 'success'
                });
            }
        });
        
        var tempo = new Array();

        //responsável pela inicialização do tempo de recarregamento do espaco
        $.fn.ativar_recarregamento = function (espaco)
        {
            if (espaco === undefined)
            {
                //ativar o recarregamento para todos os espaços expandidos
                $('.icon-plus-sign:hidden').parent('.gatilho_espaco').each(function(k,v){
                    espaco = $(v).data('espaco');
                    $(this).ativar_recarregamento(espaco)
                });
            }
            else
            {
                if((tempo[espaco] == undefined))
                {
                    tempo[espaco] = setTimeout(function () {

                        //verifica se existe algum modal visível quando as listas não devem ser repopuladas
                        if($('.modal').is(':visible'))
                        {
                            $(this).reiniciar_recarregamento(espaco);
                        }
                        else
                        {
                            $(this).popular_listas(espaco, true);
                        }

                    }, 30000, $('#progressbar_'+espaco));
                }
            }
        }

        //responsável pela reinicialização do tempo de recarregamento do espaco
        $.fn.desativar_recarregamento = function (espaco)
        {
            if (espaco === undefined)
            {
                //desativa o recarregamento para todos os espaços expandidos
                $('.icon-plus-sign:hidden').parent('.gatilho_espaco').each(function(k,v){
                    espaco = $(v).data('espaco');
                    $(this).desativar_recarregamento(espaco)
                });
            }
            else
            {
                clearInterval(tempo[espaco], $('#progressbar_'+espaco));
                tempo[espaco] = undefined;
            }
        }

        //responsável pela reinicialização do tempo de recarregamento do espaco
        $.fn.reiniciar_recarregamento = function (espaco)
        {
            $(this).desativar_recarregamento(espaco);
            $(this).ativar_recarregamento(espaco);
        }
        
        //remove operações da fila de pós atendimento
        $.fn.remover_operacoes = function ()
        {
            
            $('.limpar_canceladas').hide();
            
            $('.solicitacao_atendida').each(function ()
            {
                espaco_atual = $(this).data('espaco');
                estado_atual = $(this).data('estado');
                data_fim_utilizacao = $(this).data('fim-utilizacao');
                if(estado_atual == 'CANCELADO')
                {
                    $(this).children('.exibicao-tempo').html("");
                    $(this).children('.acoes_utilizacao').html("");
                    $('.vassoura_'+espaco_atual).show();
                }
                else
                {
                    if(data_fim_utilizacao == '')
                    {
                        $(this).children('.acoes_utilizacao').children('.alterar-tempo').remove();  
                        $(this).children('.acoes_utilizacao').children('.finalizar-tempo').remove();
                        $(this).children('.exibicao-tempo').html("");
                    }
                    else
                    {
                        $(this).children('.acoes_utilizacao').children('.cancelar-utilizacao').remove();
                    }
                }
              
            });  
        }
    
        //remove operações da fila de pós atendimento 
        $(this).remover_operacoes();
    
        //responsável pelo recarregamento das listas de tempos em tempos
        $.fn.popular_listas = function(espaco, resetar_tempo) {
            
                $('.atualizar_manualmente').find('i').addClass('icon-spin');
                $("#listagem_espaco_" + espaco).find('.loading').show();

                $.ajax({
                    type: "POST",
                    //async: false,
                    url: "https://san.navedoconhecimento.org.br/admin/solicitacao/obter_listas/"+espaco,
                    dataType: 'JSON',
                    error: function(jqXHR) {
                        if (jqXHR.status == 401)
                        {
                            location.reload();
                        }
                    }
                })
                .done(function(retorno) {
                    
                    $("#controle_conexao_" + espaco).html(retorno.conexao_ativa);
                    
                    var datatable_pendentes = $("#tabela_pendentes_" + espaco).dataTable();
                    var datatable_atendidos = $("#tabela_atendidas_" + espaco).dataTable();
                    
                    pagina_aberta_datatable_pendentes = datatable_pendentes.fnPagingInfo().iPage;
                    pagina_aberta_datatable_atendidos = datatable_atendidos.fnPagingInfo().iPage;
                    
                    //limpa as tabelas
                    datatable_pendentes.fnClearTable();
                    datatable_atendidos.fnClearTable();
                    

                    $("#tabela_pendentes_" + espaco).parent().find('#total_equipamentos_livres').html(retorno.total_equipamentos_livres);
                    $("#listagem_espaco_" + espaco).find('.dataTables_empty').html('Nenhum dado disponível na tabela.');
                    $("#listagem_espaco_" + espaco).find('.loading').hide();
                    
                    
                    //limpa as listas de equipamentos livres
                    $("select[name='equipamento_" + espaco  + "']").html('');
                    $(".listagem_equipamentos_livres_" + espaco).html('');
                    
                    //inicia a listagem de equipamentos livres: "ul"
                    $(".listagem_equipamentos_livres_" + espaco).append('<ul>');
                    
                    $.each(retorno.equipamentos, function (k, equipamento) 
                    {
                        option = '<option value="'+equipamento.id_equipamento+'">'+equipamento.nome_equipamento+'</option>';
                        eq_livre = '<li><i>'+equipamento.nome_equipamento+'</i></li>';
                        
                        //popula as listas de equipamentos livres: no "ul" e no "select"
                        $(".listagem_equipamentos_livres_" + espaco).append(eq_livre);
                        $("select[name='equipamento_" + espaco  + "']").append(option);
                    });
                    
                    //fecha a listagem de equipamentos livres: "ul"
                    $(".listagem_equipamentos_livres_" + espaco).append('</ul>');

                    //$("#pendentes_" + espaco).html('').html(retorno.pendentes);
                    //$("#atendidas_" + espaco).html('').html(retorno.atendidas);
                    
                    //popula a tabela
                    if (retorno.solicitacoes_pendentes.aaData.length > 0) {
                        datatable_pendentes.fnAddData(retorno.solicitacoes_pendentes.aaData);
                    }
                    $.each(retorno.solicitacoes_pendentes.atributos_tr, function(posicao_linha, atributos){
                        $.each(atributos, function(i, atributo){
                            linha = datatable_atendidos.fnGetNodes(posicao_linha);
                            $(linha).attr(atributo.nome, atributo.valor);
                        });
                    });

                    if (retorno.solicitacoes_atendidas.aaData.length > 0) {
                        datatable_atendidos.fnAddData(retorno.solicitacoes_atendidas.aaData);
                    }
                    $.each(retorno.solicitacoes_atendidas.atributos_tr, function(posicao_linha, atributos){
                        $.each(atributos, function(i, atributo){
                            linha = datatable_atendidos.fnGetNodes(posicao_linha);
                            $(linha).attr(atributo.nome, atributo.valor);
                        });
                    });

                    //monta a tabela
                    datatable_pendentes.fnDraw();
                    datatable_atendidos.fnDraw();

                    //troca para a página que estava selecionada
                    datatable_pendentes.fnPageChange(pagina_aberta_datatable_pendentes);
                    datatable_atendidos.fnPageChange(pagina_aberta_datatable_atendidos);

                    //reinicializa o carregamento de dados de uma solicitacao, motivo: ajax
                    $(this).carregar_dados_solicitacao();

                    if(resetar_tempo){
                        $(this).reiniciar_recarregamento(espaco);
                    }
                    
                    $('.atualizar_manualmente').find('i').removeClass('icon-spin');
                });

        };


        //responsável pela troca de equipamento de uma solicitação
        $.fn.trocar_equipamento = function() {

            equipamento_atual = $(this).data('equipamento')
            equipamento = $("select[name='equipamento_" + espaco + "']").val();

            $.ajax({
                type: "POST",
                async: false,
                url: "https://san.navedoconhecimento.org.br/admin/solicitacao/trocar_equipamento/" + solicitacao,
                dataType: 'JSON',
                data: {equipamento: equipamento},
                error: function(jqXHR) {
                    if (jqXHR.status == 401)
                    {
                        location.reload();
                    }
                }
            })
            .done(function(retorno) {

                $("#selecao-equipamento-" + espaco).modal('hide');

                if (retorno.tipo_msg == 'erro') 
                {
                    $("#msg .modal-body").html(retorno.msg);
                    $("#msg").modal();
                }
                else
                {
                    $.pnotify({
                        title: retorno.msg,
                        type: 'success'
                    });

                    parent.postMessage({'type': 'restart', value: {equipmentId: equipamento_atual, when: 0}}, '*')
                }

                //popula as listas efetivamente
                $(this).popular_listas(espaco, true);

            });

        };
        

        //responsável pela alteração de estado de uma solicitação
        $.fn.alterar_estado = function() {

            equipamento = $("select[name='equipamento_" + espaco + "']").val();

            $.ajax({
                type: "POST",
                async: false,
                url: "https://san.navedoconhecimento.org.br/admin/solicitacao/editar/" + solicitacao + "/1",
                dataType: 'JSON',
                data: {estado: estado, frequentador: frequentador, espaco: espaco, equipamento: equipamento},
                error: function(jqXHR) {
                    if (jqXHR.status == 401)
                    {
                        location.reload();
                    }
                }
            })
            .done(function(retorno) {
                if (estado == 'ATENDIDO') {
                    $("#selecao-equipamento-" + espaco).modal('hide');
                }
                else {
                    if (estado == 'CANCELADO') {
                        $("#confirmacao-cancelamento-" + espaco).modal('hide');
                    }
                }
                if (retorno.tipo_msg == 'erro') {
                    $("#msg .modal-body").html(retorno.msg);
                    $("#msg").modal();
                }
                else{
                    $.pnotify({
                        title: retorno.msg,
                        type: 'success'
                    });
                }

                //popula as listas efetivamente
                $(this).popular_listas(espaco, true);

            });

        };

        //altera o estado de uma solicitação efetivamente
        $(".enviar_alteracao").on("click", function() {

            if(operacao_solicitacao == 'troca_equipamento')
            {
                $(this).trocar_equipamento();
            }
            else
            {
                $(this).alterar_estado();
            }

        });
        
        //responsável pela troca do espaço de uma solicitação
        $.fn.trocar_espaco = function() {

            solicitacao_troca = $('.link_troca_espaco').data('solicitacao');
            espaco_origem = $('.link_troca_espaco').data('espaco-origem');
            espaco_destino = $("select[name='espaco_troca_" + espaco_origem  + "']").val();

            $.ajax({
                type: "POST",
                async: false,
                url: "https://san.navedoconhecimento.org.br/admin/solicitacao/trocar_espaco/" + solicitacao_troca,
                dataType: 'JSON',
                data: {espaco_origem: espaco_origem, espaco_destino: espaco_destino},
                error: function(jqXHR) {
                    if (jqXHR.status == 401)
                    {
                        location.reload();
                    }
                }
            })
            .done(function(retorno) {

                $("#selecao-troca-"+espaco_origem).modal('hide');

                if (retorno.tipo_msg == 'erro') {
                    $("#msg .modal-body").html(retorno.msg);
                    $("#msg").modal();
                }
                else{
                    $.pnotify({
                        title: retorno.msg,
                        type: 'success'
                    });
                    $(this).popular_listas(espaco_destino, true);
                }

                //popula as listas efetivamente
                $(this).popular_listas(espaco_origem, true);

            });

        };
        
        
        //troca o espaço de uma solicitação efetivamente
        $(".link_troca_espaco").on("click", function () {

            $(this).trocar_espaco(); 

        });
        
        
        //responsável pela alteração do tempo de uma solicitação
        $.fn.alterar_tempo = function(operacao) {

            if(operacao == undefined)
            {
                solicitacao_equipamento = $('.link_alteracao_tempo').data('equipamento');
                solicitacao_tempo = $('.link_alteracao_tempo').data('solicitacao');
                operacao = ($("input[name='operacao']").is(':checked') == true) ? 'soma' : 'subtracao'
                limite_tempo = $("input[name='limite_tempo']").val();
                //zero não deve ser permitido nas operações de soma ou subtração
                if(limite_tempo == '00:00:00')
                {
                    limite_tempo = '00:00:01';
                }
            }
            else
            {
                solicitacao_equipamento = $('.link_finalizacao_tempo').data('equipamento');
                solicitacao_tempo = $('.link_finalizacao_tempo').data('solicitacao');
                limite_tempo = '00:00:00';
            }

            $.ajax({
                type: "POST",
                async: false,
                url: "https://san.navedoconhecimento.org.br/admin/solicitacao/editar_utilizacao/"+solicitacao_tempo,
                dataType: 'JSON',
                data: {operacao: operacao, limite_tempo: limite_tempo, espaco: espaco_tempo},
                error: function(jqXHR) {
                    if (jqXHR.status == 401)
                    {
                        location.reload();
                    }
                }
            })
            .done(function(retorno) {
                //o zero apenas é possível no caso de cancelamento como definido logo acima
                if(limite_tempo != '00:00:00'){
                    $("#alteracao-tempo").modal('hide');
                }
                else{
                    $("#finalizacao-tempo").modal('hide');
                }

                if (retorno.tipo_msg == 'erro') {
                    $("#msg .modal-body").html(retorno.msg);
                    $("#msg").modal();
                }
                else{
                    $.pnotify({
                        title: retorno.msg,
                        type: 'success'
                    });

                    parent.postMessage({'type': 'restart', value: {equipmentId: solicitacao_equipamento, when: retorno.utilizacao.segundos_restantes}}, '*')
                }

                //popula as listas efetivamente
                $(this).popular_listas(espaco_tempo, true);

            });

        };
        
        //altera o limite de tempo de utilização efetivamente
        $(".link_alteracao_tempo").on("click", function () {

            $(this).alterar_tempo(); 

        });
        
        //finaliza a utilização efetivamente
        $(".link_finalizacao_tempo").on("click", function () {

            $(this).alterar_tempo('finalizacao'); 

        });
        
        
        //responsável pela alteração do tempo de uma solicitação
        $.fn.cancelar_utilizacao = function() {

            solicitacao_cancelamento = $('.link_cancelamento_utilizacao').data('solicitacao');
            espaco_cancelamento = $('.link_cancelamento_utilizacao').data('espaco');

            $.ajax({
                type: "POST",
                async: false,
                url: "https://san.navedoconhecimento.org.br/admin/solicitacao/cancelar_utilizacao/"+solicitacao_cancelamento,
                dataType: 'JSON',
                error: function(jqXHR) {
                    if (jqXHR.status == 401)
                    {
                        location.reload();
                    }
                }
            })
            .done(function(retorno) {

                $("#cancelamento-utilizacao").modal('hide');

                if (retorno.tipo_msg == 'erro') {
                    $("#msg .modal-body").html(retorno.msg);
                    $("#msg").modal();
                }
                else{
                    $.pnotify({
                        title: retorno.msg,
                        type: 'success'
                    });
                }

                //popula as listas efetivamente
                $(this).popular_listas(espaco_cancelamento, true);

            });

        };
        
        //cancela a utilização efetivamente
        $(".link_cancelamento_utilizacao").on("click", function () {

            $(this).cancelar_utilizacao(); 

        });
        
        //atribuição de link na confirmação de exclusão de um espaço
        $('.excluir').bind('click', function() {
            id_espaco = $(this).data('id');
            href = 'https://san.navedoconhecimento.org.br/admin/espaco/excluir/' + id_espaco;
            $('.link_excluir').attr('href', href);
        });

        $(".permissao_adicionar").toggle(false);
        $(".permissao_editar").toggle(false);
        $(".permissao_excluir").toggle(false);
        $(".permissao_finalizar_todas_utilizacoes").toggle(false);
          
        //atualiza todas as listagens existentes
        $(".atualizar_manualmente").on("click", function() {
            
            if (permissao_solicitacao)
            {
                //verifica se existem espaços expandidos
                if(tempo.length > 0)
                {
                    //percorre os espaços expandidos
                    for (var espaco in tempo) 
                    {
                        //atualiza as listagens de cada espaço
                        $(this).popular_listas(espaco, false);  

                    }
                }
            }
        }); 
        
        
        //responsável pela limpeza das solicitações canceladas da lista de pós atendimento
        $.fn.limpar_canceladas = function() {

            espaco_limpeza = $('.link_limpar_canceladas').data('espaco');

            $.ajax({
                type: "POST",
                async: false,
                url: "https://san.navedoconhecimento.org.br/admin/solicitacao/limpar_canceladas",
                dataType: 'JSON',
                data: {espaco: espaco_limpeza},
                error: function(jqXHR) {
                    if (jqXHR.status == 401)
                    {
                        location.reload();
                    }
                }
            })
            .done(function(retorno) {

                $("#limpeza-canceladas").modal('hide');

                if (retorno.tipo_msg == 'erro') {
                    $("#msg .modal-body").html(retorno.msg);
                    $("#msg").modal();
                }
                else{
                    $.pnotify({
                        title: retorno.msg,
                        type: 'success'
                    });
                }

                //popula as listas efetivamente
                $(this).popular_listas(espaco_limpeza, true);

            });

        }; 
        
        //limpa as solicitações canceladas efetivamente
        $(".link_limpar_canceladas").on("click", function () {

            $(this).limpar_canceladas(); 

        });
        
        //datatable da listagem de espera
        $('.datatable_pendentes').dataTable({
            "sPaginationType": "bootstrap",
            "sDom": "<'tableHeader'<l><'clearfix'f<'toolbar'><'loading'>>r>t<'tableFooter'<i><'clearfix'p>>",
            "iDisplayLength": 10,
            'bSortable': false,
            "aaSorting": [ ],
            "aoColumns": [
                {"sType": "natural", "sClass": "col_posicao", "sWidth": "20px"},
                {"sType": "natural"},
                {"sType": "natural", "sWidth": "40px"},
                {"bSortable": false, "bSearchable": false, "sClass": "col_acoes"}
            ],
            "fnDrawCallback": function() {
                //remove operações da fila de pós atendimento
                $(this).remover_operacoes();
            }
        });
        
        //datatable da listagem de concretizados
        $('.datatable_atendidos').dataTable({
            "sPaginationType": "bootstrap",
            "sDom": "<'tableHeader'<l><'clearfix'f<'loading'>>r>t<'tableFooter'<i><'clearfix'p>>",
            "iDisplayLength": 10,
            'bSortable': false,
            "aaSorting": [ ],
            "aoColumns": [
                {"sType": "natural", "sClass": "col_posicao", "sWidth": "20px"},
                {"sType": "natural"},
                {"sType": "natural", "sClass": "col_equipamento", "sWidth": "130px"},
                {"sType": "natural", "sClass": "col_center exibicao-tempo", "sWidth": "100px"},
                {"bSortable": false, "sClass": "col_center acoes_utilizacao", "sWidth": "160px"},
            ],
            "fnDrawCallback": function() {
                //remove operações da fila de pós atendimento
                $(this).remover_operacoes();
            }
        });
        
        $("div.toolbar").html('<i class="icon-laptop"></i> <span style="font-size:10px;">livres: <span id="total_equipamentos_livres"></span></span>');
        $('div.loading').html('<i class="icon-spinner icon-spin">');
        

        window.addEventListener("message", function(event) {
            if (event.data.type) {
                switch (event.data.type) {
                    case 'clientFinished':
                        console.log('Mensagem recebida: cliente finalizado!', event.data.value)
                        if (event.data.value) {
                            var id_equipamento = event.data.value

                            $.ajax({
                                type: "POST",
                                url: "https://san.navedoconhecimento.org.br/admin/solicitacao/finalizar_utilizacoes_equipamento/" + id_equipamento,
                                dataType: 'JSON',
                                error: function(jqXHR) {
                                    if (jqXHR.status == 401)
                                    {
                                        location.reload();
                                    }
                                }
                            })
                            .done(function(retorno) {
                                if (retorno.tipo_msg == 'erro') {
                                    $.pnotify({
                                        title: retorno.msg,
                                        type: 'error'
                                    });
                                }
                                else{
                                    $.pnotify({
                                        title: 'Utilização do equipamento ID='+id_equipamento+' finalizada com sucesso.',
                                        type: 'success'
                                    });
                                }
                            });
                        }
                        
                        break
                }
            }
        });
    });
</script>


